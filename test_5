@BeforeClass

public void setup()
{

WebDriver dr=new ChromeDriver();
driver.manage.window.maximize();
driver.manage.timeout.implicitlywait(Duration.ofSeconds(10));


}

@Test
public void test()
{

int a=10,b=20;
int c=a+b;
system.out.println("sum of a and b is"+c)
}

@AfterClass
public void teardown()
{

driver.close()

}


